{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": false,
      "variables": false,
      "allowNamedExports": false
    }],
    "linebreak-style": [0 ,"error", "windows"],
    "class-methods-use-this" : ["off"]
  }
}